<p id="notice"><%= notice %></p>

<h1>Listing Pedido Obras</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pedido_obras.each do |pedido_obra| %>
      <tr>
        <td><%= link_to 'Show', pedido_obra %></td>
        <td><%= link_to 'Edit', edit_pedido_obra_path(pedido_obra) %></td>
        <td><%= link_to 'Destroy', pedido_obra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

