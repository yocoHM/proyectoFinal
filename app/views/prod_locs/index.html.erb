<p id="notice"><%= notice %></p>

<!-- mensajes de error -->
<%= render 'layouts/messages' %>

<h1>Listing Prod Locs</h1>

<table>
  <thead>
    <tr>
      <th>M2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prod_locs.each do |prod_loc| %>
      <tr>
        <td><%= prod_loc.m2 %></td>
        <td><%= link_to 'Show', prod_loc %></td>
        <td><%= link_to 'Edit', edit_prod_loc_path(prod_loc) %></td>
        <td><%= link_to 'Destroy', prod_loc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prod loc', new_prod_loc_path %>
